<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义隐藏/显示密码的的EditText-->
    <declare-styleable name="HidePwEditText">
        <attr name="hideDrawable" format="reference"/>
        <attr name="showDrawable" format="reference"/>
    </declare-styleable>

    <!--个人中心常用条目控件(无icon)-->
    <declare-styleable name="LabelIndicatorView">
        <attr name="label_title" format="string"/>
        <attr name="label_tips" format="string"/>
        <attr name="label_Indicator" format="reference"/>
    </declare-styleable>

    <!--顶部悬停控件-->
    <declare-styleable name="StickyNavLayout">
        <!--默认是否悬停tab-->
        <attr name="isStickNav" format="boolean"/>
        <!--距离悬浮的位置-->
        <attr name="stickOffset" format="dimension"/>
    </declare-styleable>

    <!--************ TipsLoadingView ***********-->
    <declare-styleable name="TipsLoadingView">
        <attr name="loading_view_size" format="dimension"/>
        <attr name="android:color"/>
    </declare-styleable>
    <attr name="LoadingStyle" format="reference"/>

    <declare-styleable name="IconLabelIndicatorView">
        <attr name="viewIcon" format="reference" />
        <attr name="viewLabel" format="string" />
        <attr name="viewIndicator" format="reference" />
    </declare-styleable>
</resources>